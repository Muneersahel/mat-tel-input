{
  "name": "mat-tel-input",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/mat-tel-input/src",
  "prefix": "",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/mat-tel-input/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mat-tel-input/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mat-tel-input/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/mat-tel-input/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/mat-tel-input"
      }
    }
  },
  "release": {
    "version": {
      "preVersionCommand": "npm run copy-files && npx nx build mat-tel-input",
      "generatorOptions": {
        "packageRoot": "dist/libs/mat-tel-input"
      }
    },
    "changelog": {
      "workspaceChangelog": {
        "createRelease": "github"
      }
    }
  }
}
